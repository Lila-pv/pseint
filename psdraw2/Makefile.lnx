OBJS_DIR=../temp/drw2_lnx_rls
GPP=g++ -fshow-column -D_USE_TEXTURES
GCC=gcc
CXXFLAGS= -O2
LIBS= -s -lGL -lGLU -lglut -lpng

all: ${OBJS_DIR} ../bin/psdraw2

include Makefile.common

clean:
	rm -rf ${OBJS} ../bin/psdraw2

../bin/psdraw2: ${OBJS}
	${GPP} ${OBJS} ${LIBS} -o $@

${OBJS_DIR}:
	mkdir ${OBJS_DIR}

